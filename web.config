<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="iisnode" path="index1.js" verb="*" modules="iisnode"/>
    </handlers>
    <rewrite>
      <rules>
        <rule name="NodeRouting" stopProcessing="true">
          <match url=".*"/>
          <action type="Rewrite" url="index1.js"/>
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>
